{
  "name": "locutus",
  "version": "2.0.0",
  "license": "MIT",
  "description": "Locutus assimilaties other languages' standard libraries to JavaScript for fun and educational purposes",
  "homepage": "http://locutusjs.io",
  "author": "Kevin van Zonneveld <kevin@vanzonneveld.net>",
  "main": "src/index.js",
  "engines": {
    "node": ">= 0.10.0"
  },
  "devDependencies": {
    "async": "2.0.0-rc.3",
    "chai": "3.5.0",
    "deep-equal": "1.0.1",
    "depurar": "0.2.0",
    "eslint": "2.8.0",
    "eslint-config-standard": "5.1.0",
    "eslint-plugin-standard": "1.3.2",
    "fakefile": "0.0.8",
    "globby": "4.0.0",
    "js-yaml": "3.6.0",
    "lodash": "4.11.1",
    "mkdirp": "0.5.1",
    "mocha": "2.4.5",
    "rimraf": "2.5.2",
    "send": "0.13.2",
    "underscore": "1.8.3"
  },
  "keywords": [
    "php",
    "js",
    "locutus"
  ],
  "files": [
    "functions"
  ],
  "bugs": {
    "url": "https://github.com/kvz/locutus/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kvz/locutus.git"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "build": "npm run reindex",
    "playground:start": "cd test/browser && node server.js",
    "deploy": "npm run website:deploy",
    "fix": "eslint src test --fix",
    "lint": "eslint src",
    "release:major": "env SEMANTIC=major npm run release",
    "release:minor": "env SEMANTIC=minor npm run release",
    "release:patch": "env SEMANTIC=patch npm run release",
    "release": "git commit CHANGELOG.md -m 'Update CHANGELOG.md' && npm version ${SEMANTIC:-patch} -m \"Release %s\" && git commit test/fixture/*.stdio -m 'Update version' && git push --tags && git push && npm publish",
    "test:languages": "npm run test:write && mocha --reporter spec test/languages/**/*.js",
    "test:module": "node test/npm/npm.js",
    "test:utils": "mocha --reporter spec",
    "test:write": "rimraf test/languages && node src/_util/cli.js writetests",
    "test": "npm run test:utils && npm run test:languages && npm run test:module",
    "reindex": "./scripts/reindex.sh src",
    "website:deploy": "npm run website:inject && cd website && npm run build && npm run deploy",
    "website:inject": "rimraf website/_functions/* && node src/_util/cli.js injectweb",
    "website:install": "cd website && npm install",
    "website:start": "cd website && npm run start"
  },
  "bin": {
    "node src/_util/cli.js": "src/_util/cli.js"
  }
}
